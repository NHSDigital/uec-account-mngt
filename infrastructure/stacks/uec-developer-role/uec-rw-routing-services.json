{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "RoutingPermissionsGeneral",
            "Effect": "Allow",
            "Action": [
                "wafv2:*",
                "route53:*",
                "acm:*",
                "route53domains:ListDomains"
            ],
            "Resource": "*"
        },
        {
            "Sid": "Ec2Permissions",
            "Effect": "Allow",
            "Action": "ec2:*",
            "Resource": "*"
        }
    ]
}
