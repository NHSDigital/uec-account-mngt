{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowSundry",
            "Effect": "Allow",
            "Action": [
                "schemas:*",
                "scheduler:*",
                "apigateway:*"
            ],
            "Resource": "*"
        },
        {
            "Sid": "AllowApiGateway",
            "Effect": "Allow",
            "Action": "apigateway:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowSchemas",
            "Effect": "Allow",
            "Action": [
                "pipes:ListPipes",
                "schemas:CreateDiscoverer",
                "schemas:GetDiscoveredSchema",
                "scheduler:ListSchedules",
                "scheduler:ListScheduleGroups"
            ],
            "Resource": "*"
        },
        {
            "Sid": "AllowPipes",
            "Effect": "Allow",
            "Action": "pipes:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowSns",
            "Effect": "Allow",
            "Action": "sns:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowLambda",
            "Effect": "Allow",
            "Action": "lambda:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowStates",
            "Effect": "Allow",
            "Action": "states:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowCloudfront",
            "Effect": "Allow",
            "Action": "cloudfront:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowSqs",
            "Effect": "Allow",
            "Action": "sqs:*",
            "Resource": "*"
        },
        {
            "Sid": "AllowEvents",
            "Effect": "Allow",
            "Action": "events:*",
            "Resource": "*"
        },
        {
          "Sid": "AllowCloudshell",
          "Effect": "Allow",
          "Action": "cloudshell:*",
          "Resource": "*"
        },
        {
          "Sid": "AllowEcs",
          "Effect": "Allow",
          "Action": "ecs:*",
          "Resource": "*"
        },
        {
          "Sid": "AllowOpenSearch",
          "Effect": "Allow",
          "Action": [
              "osis:*",
              "opensearch:*",
              "aoss:*"
          ],
          "Resource": "*"
      }
    ]
}
